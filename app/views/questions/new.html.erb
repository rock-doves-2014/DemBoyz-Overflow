<%= form_for(@question) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :image_url %>
  <%= f.text_field :image %>
  <%= f.label :content %>
  <%= f.text_area :content %>
  <%= f.fields_for :tag do |f| %>
    <%= f.label :tags %>
    <%= f.text_field :title %>
  <% end %>
  <%= f.submit "Create Question" %>
<% end %>

